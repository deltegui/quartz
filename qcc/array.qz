import 'stdio';
import 'stdconv';

var a: []Number = []Number{
    0,
    1,
    5,
    123
};

typeof(a);

var b = cast<[]Number>(a);

b.push(2);

fn each(arr: []Any, operate: (Number, Any): Void) {
    var len = arr.length();
    for (var i = 0; i < len; i = i + 1) {
        operate(i, arr.get(i));
    }
}

fn print_number(i: Number, ele: Any) {
    var c = cast<Number>(ele);
    println("[" + ntos(i) + "] " + ntos(c));
}

each(b, print_number);
