import 'stdio';

fn tell_me_if_five(n: Number) {
    if (n > 5) {
        println("Is greater than five");
    } else if (n < 5) {
        println("Is lower than five");
    } else {
        println("Is five");
    }
}

tell_me_if_five(5);
tell_me_if_five(2);
tell_me_if_five(6);

