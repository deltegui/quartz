import 'stdio';
import 'stdconv';

fn do(): (): Void {
    class Human {
        var name: String;
        var age: Number;

        pub fn init(name: String, age: Number) {
            self.name = name;
            self.age = age;
        }

        pub fn say_hi() {
            println("Hello, im " + self.name + " and im " + ntos(self.age) + " years old!");
        }
    }
    var diego: Human = new Human('diego', 24);
    return diego.say_hi;
}

do()();