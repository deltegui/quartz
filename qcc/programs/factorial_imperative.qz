import 'stdio';
import 'stdconv';

fn factorial(n: Number): Number {
    var result = 1;
    while (n>1) {
        result = result * n;
        n = n - 1;
    }
    return result;
}

fn println_number(n: Number) {
    println(ntos(n));
}

println_number(factorial(7));
println_number(factorial(10));
println_number(factorial(55));
