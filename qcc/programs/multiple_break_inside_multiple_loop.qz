fn lo(a: Number) {
    for (;;) {
        print a;
        if (a <= 6) {
            while(true) {
                if (a <= 3) {
                    break;
                }
                a = a * 2;
                print a;
                if (a >= 2000) {
                    break;
                }
            }
        }
        if (a <= 10) {
            break;
        }
        if (a % 5 == 0) {
            break;
        }
        a = a + 1;
    }
}

lo(3);
lo(6);
lo(7);
lo(11);
